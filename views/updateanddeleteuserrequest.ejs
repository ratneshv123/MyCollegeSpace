              <a class="btn float-right">Pending Requests-:</a>
            </h5>
          </div>
          <div id="collapseThree" class="collapse" aria-labelledby="headingThree" data-parent="#accordion">
            <div class="card-body">
                <% if(typeof usern !=='undefined') { %>
                    <% usern.forEach((user)=> { %>
                        
                        <form  method="post">
                            <h5 class="card-title">Name-<%=user.name%> , Semester-<%=user.id%>  &nbsp;
                                <input type="hidden" name="delname" value="<%=user.name%>">
                                <input type="hidden" name="delid" value="<%=user.id%>">
                            <button class="btn btn-danger" type="submit" formaction="/notesacceptrequest">Accept</button>
                            <button class="btn btn-danger" type="submit" formaction="/notesdeleterequest">Reject</button>
                        </form>

                    </h5>
                        
                      <embed src="/uploading/notes/<%= user.name%>.pdf" type="application/pdf" width="100%" height="700px" /> 
                      <br>      
                      <br> 
                      <hr>  
                    <% }) %>
                <%}%> 
 </div>
          </div>
        </div>
      </div>
   
