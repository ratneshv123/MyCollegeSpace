  <!-- only for uploading files and working condition  -->
  <button type="button" class="btn btn-primary btn-lg btn-block">User Requests</button>
    <hr>
  
      <div id="accordion">
        <div class="card">
          <div class="card-header" id="headingOne">
            <h5 class="mb-0">
             
                <button class="btn btn-link" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                    All Papers Request
                  </button>
              <button class="btn btn-success float-right" ><%=userp.length%></button>
              <a class="btn float-right">Pending Requests-:</a>
            </h5>
          </div>
      
          <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
            <div class="card-body">
                <% if(typeof userp !=='undefined') { %>
                    <% userp.forEach((user)=> { %>
                        
                        <form  method="post">
                            <h5 class="card-title">Name-<%=user.name%> , Semester-<%=user.id%>  &nbsp;
                                <input type="hidden" name="delname" value="<%=user.name%>">
                                <input type="hidden" name="delid" value="<%=user.id%>">
                            <button class="btn btn-danger" type="submit" formaction="/paperacceptrequest">Accept</button>
                            <button class="btn btn-danger" type="submit" formaction="/paperdeleterequest">Reject</button>
                        </form>
                    </h5> 
                      <embed src="/uploading/mcapapers/<%= user.name%>.pdf" type="application/pdf" width="100%" height="700px" /> 
                      <br>      
                      <br> 
                      <hr>  
                    <% }) %>
                <%}%> 


            </div>
          </div>
        </div>
        <div class="card">
          <div class="card-header" id="headingTwo">
            <h5 class="mb-0">
              <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                All Books Request
              </button>
              <button class="btn btn-success float-right" ><%=userb.length%></button>
              <a class="btn float-right">Pending Requests-:</a>
            </h5>
          </div>
          <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion">
            <div class="card-body">
                <% if(typeof userb !=='undefined') { %>
                    <% userb.forEach((user)=> { %>
                        
                        <form  method="post">
                            <h5 class="card-title">Name-<%=user.name%> , Semester-<%=user.id%>  &nbsp;
                                <input type="hidden" name="delname" value="<%=user.name%>">
                                <input type="hidden" name="delid" value="<%=user.id%>">
                            <button class="btn btn-danger" type="submit" formaction="/booksacceptrequest">Accept</button>
                            <button class="btn btn-danger" type="submit" formaction="/booksdeleterequest">Reject</button>
                        </form>
                    </h5>
                        
                      <embed src="/uploading/books/<%= user.name%>.pdf" type="application/pdf" width="100%" height="700px" /> 
                      <br>      
                      <br> 
                      <hr>  
                    <% }) %>
                <%}%> 

            </div>
          </div>
        </div>
        <div class="card">
          <div class="card-header" id="headingThree">
            <h5 class="mb-0">
              <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
