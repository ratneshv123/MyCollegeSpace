        <!-- Optional JavaScript -->
        <!-- jQuery first, then Popper.js, then Bootstrap JS -->

  <!-- only for uploading files and working condition  -->
  <button type="button" class="btn btn-info btn-lg btn-block">Books</button>
    <hr>


    <div class="row">
        <div class="col-sm-6">
          <div class="card">
              <div class="card-body">
                <div class="updateanddeletepapers">
                    <br>
                        <h1 class="text-center">Upload Books</h1>
                        <form action="/upbooks" method="post" enctype="multipart/form-data">
                          <div class="form-group">
                              <label for="file1">MCA Section</label>
                              <input type="file" name="file" required id="" class="form-control">
                              <input type="text" placeholder="Semester" required id="" class="form-control" name="paperid">
                          </div>
                          <div class="form-group">
                              <button class="btn btn-info btn-block">
                                  Upload Book
                              </button>
                          </div>
                      </form>
                      <br> 
                      <br>
      
      
      
                    
                </div>    
    
              </div>      
          </div>
        </div>
        <div class="col-sm-6">
          <div class="card">
            <div class="card-body">
              
                <h5>Notes</h5>
                <form id="tableForm" action="/watchbooks" method="post">
                    <select class="btn btn-danger" data-style="btn-info" name="semester">
                            <option name="mca" value="1">MCA-Semester 1</option>
                            <option name="mca" value="2">MCA-Semester 2</option>
                            <option name="mca" value="3">MCA-Semester 3</option>
                            <option name="mca" value="4">MCA-Semester 4</option>
                            <option name="mca" value="5">MCA-Semester 5</option>
                            <option name="mca" value="6">MCA-Semester 6</option>
                    </select>
                    <input type="hidden" name="course" value="mca">
                    <input type="submit" class="btn btn-info" />
                </form>
                
                <hr>
            <% if(typeof users !=='undefined') { %>
                <% users.forEach((user)=> { %>
                    
                    <form action="/deletethisbook" method="post">
                        <h5 class="card-title">Name-<%=user.name%> , Semester-<%=user.id%>  &nbsp;
                            <input type="hidden" name="delbookname" value="<%=user.name%>">
                            <input type="hidden" name="delbookid" value="<%=user.id%>">
                            <input type="hidden" name="delbookprimeid" value="<%=user.bookid%>">
                        <button class="btn btn-danger" type="submit">Delete</button>
                    </form>
                </h5>
                    
                  <embed src="/uploading/books/<%= user.name%>.pdf" type="application/pdf" width="100%" height="700px" /> 
                  <br>      
                  <br> 
                  <hr>  
                <% }) %>
            <%}%> 

        </div>
          </div>
        </div>
      </div>



  
   
    <script src="usersigninjs.js"></script>
