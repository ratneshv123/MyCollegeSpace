  <div class="container">
    <div class="row">
      <div class="col-md">
        <div class="info-box">
            <button type="button" class="btn btn-dark btn-lg btn-block">Request</button>
            <br>

            <div class="row">
                <div class="col-sm-10">
                  
                    <div class="card">
                        <div class="card-body">
                          <h5 class="card-title">Upload Papers</h5>
                          <p class="card-text">Here you can upload the papers only in pdf format.</p>
                          <form action="/upuserpapers" method="post" enctype="multipart/form-data">
                            <div class="form-group">
                                <label for="file1">MCA Section</label>
                                <input type="file" name="file" required id="" class="form-control">
                                <input type="text" placeholder="Semester" required id="" class="form-control" name="paperid">
                            </div>
                            <div class="form-group">
                                <input type="hidden" name="asfd" value="papers">
                                <button class="btn btn-primary btn-block">
                                    Upload Papers
                                </button>
                            </div>
                        </form>
                        </div>
                      </div>
                      <hr>
                      <div class="card">
                        <div class="card-body">
                          <h5 class="card-title">Upload Notes</h5>
                          <p class="card-text">Here you can upload Notes only in pdf format.</p>
                          <form action="/upusernotes" method="post" enctype="multipart/form-data">
                            <div class="form-group">
                                <label for="file1">MCA Section</label>
                                <input type="file" name="file" required id="" class="form-control">
                                <input type="text" placeholder="Semester" required id="" class="form-control" name="paperid">
                            </div>
                            <div class="form-group">
                                <button class="btn btn-primary btn-block">
                                    Upload Notes
                                </button>
                            </div>
                        </form>
                        </div>
                      </div>
                      <hr>
                      <div class="card" >
                        <div class="card-body">
                          <h5 class="card-title">Upload Books</h5>
                          <p class="card-text">Here you can upload Book only in pdf format.</p>
                          <form action="/upuserbooks" method="post" enctype="multipart/form-data">
                            <div class="form-group">
                                <label for="file1">MCA Section</label>
                                <input type="file" name="file" required id="" class="form-control">
                                <input type="text" placeholder="Semester" required id="" class="form-control" name="paperid">
                            </div>
                            <div class="form-group">
                                <button class="btn btn-primary btn-block">
                                    Upload Books
                                </button>
                            </div>
                        </form>
                        </div>
                      </div>
                      
                </div>
              </div>


        </div>
      </div>
