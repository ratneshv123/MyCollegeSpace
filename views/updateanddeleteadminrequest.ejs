  <!-- only for uploading files and working condition  -->
  
  <section>
    <div class="card" style="margin-top: 40px; margin-bottom: 40px;">
        <h5 class="card-header" style="background-color: rgb(127, 129, 199);">All Request</h5>
        <div class="card-body">
            <table class="table table-dark" style="background-color: rgba(67, 98, 182, 0.384);">
                <thead>
                  <tr>
                    <th scope="col">ID</th>
                    <th scope="col">NAME</th>
                    <th scope="col">USERNAME</th>
                    <th scope="col">E-MAIL</th>
                    <th scope="col">CONTACT</th>
                    <th scope="col">ACCEPT</th>
                    <th scope="col">REJECT</th>
                  </tr>
                </thead>
                <tbody>
                    <% if(typeof users !=='undefined') { %>
                        <% users.forEach((user)=> { %>
                            <tr>
                                <form method="POST">
                                    <td id = "style_id"><input type="text" style="width: 25px;text-align: center;" value="<%= user.id %>" name="id" readonly></td>
                                    <td id = "style_id"><input type="text" style="width: 120px;" value="<%= user.name%>" name="name" readonly></td>
                                    <td id = "style_id"><input type="text" value="<%= user.username%>" name="username" readonly></td>
                                    <td id = "style_id"><input type="text" style=" width: 200px;" name="email" value="<%= user.email%>" readonly></td>
                                    <td id = "style_id"><input type="text" style=" width: 120px;" value="<%= user.mobileno%>" name="contact" readonly></td>
                                    <td id = "style_id"><button type="submit" formaction="/acceptingadminrequest" class="btn btn-danger">Accept</button></td>
                                    <td id = "style_id"><button type="submit" formaction="/rejectingadminrequest" class="btn btn-primary">Reject</button></td>
                                </form>
                            </tr>
                        <% }) %>
                    <% } %>
                </tbody>
            </table>
        </div>
    </div>        
</section>
   
